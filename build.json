{
  "plugins": [
    [
      "build-plugin-fusion",
      {
        "themePackage": [
          {
            "name": "@alifd/theme-design-pro"
          },
          {
            "name": "@alifd/theme-1"
          },
          {
            "name": "@alifd/theme-4",
            "default": true
          }
        ]
      }
    ],
    [
      "build-plugin-moment-locales",
      {
        "locales": [
          "zh-cn"
        ]
      }
    ]
  ],
  "proxy": {
    "/api": {
      "enable": true,
      "target": "http://127.0.0.1:7001/"
    }
  }
}
